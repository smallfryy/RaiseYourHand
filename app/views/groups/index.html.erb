
<div class="panel panel-default">
  <div class="panel-heading text-center"><h1>Groups</h1></div>
    <div class="list-group">
    <ul>
      <% @groups.each do |group| %>
        <li class="list-group-item"><p><%= link_to(group.name, group) << " - " << group.description%></p></li>
      <% end %>
    </ul>
    </div>
</div>
