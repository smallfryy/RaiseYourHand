<div class="panel panel-default">
  <div class="panel-heading text-center"><h1>Trending Tags</h1></div>
    <div class="list-group">
      <ul>
        <% @tags.each do |tag| %>
          <li class="list-group-item"><p><%= link_to tag.name, tag %></p></li>
        <% end %>
      </ul>
    </div>
  </div>
</div>
