<div class="panel panel-default">
  <div class="panel-heading text-center"><h1>Tags</h1></div>
    <div class="list-group">
      <br><ul>
        <% @tags.sort{ |a,b| a.name.downcase <=> b.name.downcase }.each do |tag| %>
          <li class="list-group-item"><p><%= link_to tag.name.capitalize, tag %></p></li>
        <% end %>
      </ul>
    </div>
  </div>
</div>

